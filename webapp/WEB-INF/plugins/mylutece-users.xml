<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plug-in>
    <name>mylutece-users</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>module.mylutece.users.module.description</description>
    <provider>module.mylutece.users.module.provider</provider>
    <provider-url>https://github.com/lutece-platform</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2019 Mairie de paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>MYLUTECE_USERS_MANAGEMENT</feature-id>
            <feature-title>module.mylutece.users.adminFeature.myLuteceUsersManagement.name</feature-title>
            <feature-description>module.mylutece.users.adminFeature.myLuteceUsersManagement.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/mylutece/modules/users/ManageLocalUsers.jsp</feature-url>
